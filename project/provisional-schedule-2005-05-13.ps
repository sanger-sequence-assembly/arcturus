    exch		% dx i
    10 mm mul		% dx x0
    exch		% x0 dx
    0 exch		% x0 y0 dx
    2 mm		% x0 y0 dx dy
    4 copy		% x0 y0 dx dy x0 y0 dx dy
    rectfill		% x0 y0 dx dy
    pop exch pop add	% x0+dx
    -2 mm		% x0+dx dy
    newpath moveto	% --
} def

/dy -4.5 mm def

/NL { 0 dy translate} def

/done { 1 mm 2 mm rmoveto darkgreen (DONE) show } def
/ongoing { 1 mm 2 mm rmoveto blue (ONGOING) show } def
/notstarted { 1 mm 2 mm rmoveto red (NOT STARTED) show } def

